      <!-- CATS AND NEWS -->
      <% @stories.each_with_index do |story, index| %>  
      <section id="photocard" class="e-bg-light bg-color01 photocard-block pt-section" data-name="Photocard">
      <% if index.even? %>
         <div>
            <div class="photocard-wrap animated fadeIn clearfix animation-action">
               <div class="photocard-image-wrap  column-half column-float-left">
                  <div class="photocard-image-container"
                       style=<%=@gifs[index]%>></div>
               </div>
               <div class="photocard-content-wrap dark column-float-right column-half" data-stellar-background-ratio="0.5">
                  <div class="photocard-contents-inner text-align-center wow fadeInRight" data-wow-delay="0.4s" data-wow-offset="10">
                     <div class="heading-block">
                        <h2 class="photocard-title font-accident-two-bold-italic ">
                           <%= story[:headline] %>
                        </h2>
                     </div>
                     
                     <div class="dividewhite3"></div>
                     <a role="button" href="<%= story[:link] %>" class="btn btn-default btn-gr" target="_blank">Read more</a>
                  </div>
               </div>
            </div>
         </div>
      <% else %>
         <div class="clearfix">
